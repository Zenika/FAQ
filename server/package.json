{
  "name": "server",
  "version": "1.0.0",
  "dependencies": {
    "algoliasearch": "^3.30.0",
    "emojilib": "^2.3.0",
    "express": "^4.16.3",
    "express-force-https": "^1.0.0",
    "express-jwt": "^5.3.1",
    "form-data": "^2.3.2",
    "graphql-yoga": "^1.17.4",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.3.0",
    "jwks-rsa": "^1.3.0",
    "mjml": "^4.3.1",
    "node-fetch": "^2.2.0",
    "path": "^0.12.7",
    "prisma": "^1.26.6",
    "prisma-binding": "^2.3.2",
    "prisma-multi-tenant": "^1.0.0",
    "showdown": "^1.8.6",
    "showdown-xss-filter": "^0.2.0",
    "slugify": "^1.3.1"
  },
  "scripts": {
    "start": "dotenv -e .env.local node src/index.js",
    "dev": "dotenv -e .env.local nodemon src/index.js",
    "deploy": "cd scripts/prisma_deploy_all && dotenv -e ../../.env.local node index.js",
    "new_service": "cd scripts/prisma_new_service && dotenv -e ../../.env.local node index.js",
    "new_group": "cd scripts/faq_new_group && dotenv -e ../../.env.local node index.js",
    "generate_schema": "prisma generate",
    "lint": "eslint src --ext js",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier src/**/*",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "local_containers": "cd prisma/ && dotenv -e ../.env.local docker-compose up",
    "token": "dotenv -e .env.local bash scripts/prisma_token/token.sh"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "dotenv-cli": "^1.4.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "prisma-yml": "^1.17.1"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "singleQuote": true
  }
}
