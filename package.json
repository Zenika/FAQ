{
  "name": "faq",
  "version": "0.3.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.1.1",
    "auth0-js": "^9.2.3",
    "emoji-dictionary": "^1.0.9",
    "font-awesome": "^4.7.0",
    "graphql": "^0.13.1",
    "react": "^16.2.0",
    "react-apollo": "^2.1.0-beta.2",
    "react-dom": "^16.2.0",
    "react-markdown": "^3.2.0",
    "react-mde": "^2.3.4",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "react-toolbox": "^2.0.0-beta.12",
    "showdown": "^1.8.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build_without_map": "react-scripts build && rm build/static/js/*.map",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src --fix",
    "deploy_now": "now build --name faq-zenika --public --token $NOW_TOKEN && now alias faq-zenika --token $NOW_TOKEN && now rm faq-zenika --safe --yes --token $NOW_TOKEN",
    "deploy_graphcool": "cd server && npm run login && npm run deploy_prod",
    "deploy": "npm run build_without_map && npm run deploy_now && npm run deploy_graphcool"
  },
  "devDependencies": {
    "dotenv": "^5.0.0",
    "dotenv-cli": "^1.4.0",
    "eslint": "^4.18.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "graphcool": "^0.11.5",
    "now": "^9.3.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "eslintIgnore": [
    "src/toolbox"
  ],
  "now": {
    "name": "faq-zenika",
    "alias": "faq-zenika"
  }
}
